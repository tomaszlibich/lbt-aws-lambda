{
  "name": "lbt-aws-lambda",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "ci": "npm test && npm run typecheck && npm run package",
    "package": "npm run build && node scripts/package.mjs",
    "dev:invoke": "npm run build && node --enable-source-maps local/local-invoke.js",
    "lint": "echo \"(optional) add eslint later\"",
    "test": "node --test",
    "typecheck": "tsc -p tsconfig.build.json --noEmit"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/node": "^24.0.0",
    "dotenv": "^17.2.3",
    "typescript": "^5.7.0"
  }
}
